#include <bits/stdc++.h>
using namespace std;

string S1;
string S2;
int main(){
    bool anagrama=false;
    getline(cin,S1);
    getline(cin,S2);
    for (int i=0;i<S1.length();i++){
        anagrama=false;
        for(int j=0;j<S2.length();j++){
            if(S2[j]==S1[i]){
                anagrama=true;
            }
            else{

            }
        }
        if(anagrama==false){
            i=S1.length();
        }
    }
    if(anagrama==true){
        cout<<"ANAGRAMA!";
    }
    else{
        cout<<"No :(";
    }
}
